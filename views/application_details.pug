extends base.pug

block content 

  h2 #{application.name}

  div(class="panel panel-default")
    div(class="panel-heading")
      strong Details
    table(class="wb-tables table table-bordered table-hover" data-wb-tables='{ "ordering" : false }')
      tbody
        tr 
          th Application Name:
          td #{application.name}
        tr 
          th Application ID: 
          td #{application.id}